#import "C4Canvas.h"

C4Voice *princess;

@implementation C4Canvas
-(void)setup {
princess = [[[C4Voice alloc] initWithVoice:PRINCESS] retain];
[princess setVolume:0.05];
[princess speak:@"where are my glass slippers?"];
}
@end